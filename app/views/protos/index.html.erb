<p id="notice"><%= notice %></p>

<h1>Protos</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @protos.each do |proto| %>
      <tr>
        <td><%= proto.title %></td>
        <td><%= proto.user_id %></td>
        <td><%= link_to 'Show', proto %></td>
        <td><%= link_to 'Edit', edit_proto_path(proto) %></td>
        <td><%= link_to 'Destroy', proto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proto', new_proto_path %>
